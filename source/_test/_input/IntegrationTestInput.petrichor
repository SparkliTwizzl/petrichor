#: top-level line comment

module-options: #: inline comment in region name
{
	#: line comment in metadata region
	default-icon: ../_input/IconDefault.ico #: inline comment in metadata region body
	suspend-icon: "../_input/IconSuspend.ico"
	reload-shortcut: #r
	suspend-shortcut: #s
}

entries:
{
	#: line comment before entries in entries region
	{
		#Sam @sm #: inline comment in entries region body
		#Sammy @smy
		$they/them
		&-> a person
	}
	
	#: line comment between entries in entries region
	{
		#ALEX @AX
		$IT/ITS
		&[A PERSON TOO]
	}
}

templates:
{
	#: line comment in templates region
	::\@@:: \## #: inline comment in templates region body
	::\@@-:: \## \$($)
	::\@@/:: \## \&&
	::\@@-/:: \## \$($) \&&
}
