#: top-level line comment

metadata: #: inline comment in region name
{
	#: line comment in metadata region body
	minimum-version: 0.9 #: inline comment in metadata region body
}

module-options:
{
	#: line comment in module options region body
	default-icon: ../_input/IconDefault.ico #: inline comment in module options region body
	suspend-icon: "../_input/IconSuspend.ico"
	reload-shortcut: [tab] [and] r
	suspend-shortcut: [win]s
}

entries:
{
	#: line comment before entries in entries region body
	entry: #: inline comment in entries region body
	{
		#: line comment in entry region body
		id: 1234 #: inline comment in entry region body
		last-name: Smith @s
		name: Sam @sm
		name: Sammy @smy
		pronoun: they/them
		color: black
		decoration: -> a person
	}
	
	entry:
	{
		id: 4321
		name: ALEX @AX
	}
}

templates:
{
	#: line comment in templates region body
	template: [tag][last-tag] :: ` \[[id]\] [name] [last-name] ([pronoun]) [decoration] ` #: inline comment in templates region body
}
